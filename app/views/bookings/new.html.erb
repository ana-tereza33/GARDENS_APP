<h1>Bookings#new</h1>
<p>Find me in app/views/bookings/new.html.erb</p>

<%= simple_form_for [@garden, @booking] do |f| %>
  <div>
  <%= f.label :start_date %>
  <%= f.text_field :start_date, placeholder:"DD/MM/YY" %>
  </div>

  <br />

  <div>
  <%= f.label :end_date %>
  <%= f.text_field :end_date, placeholder:"DD/MM/YY" %>
  </div>

  <br />

  <%= f.submit "Create", class: "btn btn-primary"%>
<% end %>
